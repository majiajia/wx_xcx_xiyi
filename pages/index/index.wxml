<view style='display:flex;flex-direction:column;justicy-content:center;align-items:center;width:100%;background-color:#F7F7F7;padding-bottom:50rpx;'>
    <!-- 最上面显示康洁的图片logo -->
    <image style='width:100%;height:240rpx;' src='../../imgs/kangel_logo.jpg'></image>
    
    <!-- 在一张图片上显示三种活动  -->
    <view style="width:100%;height:200rpx;background-image:url('data:image/png;base64,');display:flex;flex-direction:column;justify-content:center;align-items:center;" bindtap='nav_to_huodong'>
        <!-- <image style='width:100%;height:120rpx;' src='../../imgs/huodong.png'></image> -->

        <block wx:if="{{cur_active_type == 1}}">
            <view wx:for="{{huodong_cfg_list}}" wx:for-item="huodong_cfg_item" style="color:white;font-weight:bold;">
                <block wx:if="{{huodong_cfg_item.pay_real > 0}}">
                    <text>充{{huodong_cfg_item.pay_real}}送{{huodong_cfg_item.gift}} = {{huodong_cfg_item.discount}}折 + 返{{huodong_cfg_item.fan_num}}折</text>
                </block>
            </view>
        </block>
        <block wx:elif="{{cur_active_type == 2}}">
            <view wx:for="{{huodong_cfg_list}}" wx:for-item="huodong_cfg_item" style="color:white;font-weight:bold;width:100%;">
                <block wx:if="{{huodong_cfg_item.pay > 0}}">
                    <view style="display:flex;flex-direction:row;width:100%;">
                        <view style="width:50%;display:flex;flex-direction:row;justify-content:flex-end;margin-right:40rpx;">
                            <text>充值{{huodong_cfg_item.pay}}</text>
                        </view>
                        <view style="width:50%;">
                            <text>送{{huodong_cfg_item.fan_num}}</text>
                        </view>
                    </view>
                    <text>  </text>
                </block>
            </view>
        </block>
        
    </view>
    
    <!-- 所洗的衣服的种类 -->
    <view style='width:100%;padding-top:20rpx;padding-bottom:12rpx;background-color:white;'>
        <view style='display:flex;flex-direction:row;justify-content:center;align-items:center;'>
            <text>可提供的服务</text>
        </view>
        
        <view style='width:100%;'>
            <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;padding-top:16rpx;padding-bottom:16rpx;' >
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu' data-type="1">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/cloth.png'></image>
                    <text>服饰</text>
                </view>
                <text style='color:grey;'>|</text>
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu'  data-type="2">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/jiaju.png'></image>
                    <text>家居</text>
                </view>
            </view>
            
            <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;padding-top:16rpx;padding-bottom:16rpx;'>
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu'  data-type="3">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/shoe.png'></image>
                    <text>鞋类</text>
                </view>
                <text style='color:grey;'>|</text>
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu'  data-type="4">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/bao.png'></image>
                    <text>包类</text>
                </view>
            </view>
            
            <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;padding-top:16rpx;padding-bottom:16rpx;'>
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu'  data-type="5">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/piyi.png'></image>
                    <text>皮草</text>
                </view>
                <text style='color:grey;'>|</text>
                <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;' bindtap='nav_to_jiamu'  data-type="6">
                    <image style='width:48rpx;height:48rpx;' src='../../imgs/cloth/ranse.png'></image>
                    <text>染色</text>
                </view>
            </view>
        </view>
    </view>

    <view style="display:flex;flex-direction:row;justify-content:center;align-items:center;font-size:28rpx;margin-top:24rpx;color:grey;">
        <text>提供上门取衣服务,让您更方便,每天下午5:00-8:00取衣</text>
    </view>
    <!-- 预约上门按钮 用户点击后,跳转到用户填写自己的 -->
    <view style='display:flex;flex-direction:column;justify-content:center;align-items:center;border:2rpx solid #10B9F4;border-radius:20rpx;padding:10rpx;margin-top:20rpx;background-color:white;' bindtap='nav_to_yuding'>
        <text style='color:#10B9F4;font-size:36rpx;'>预约上门取衣</text>
        <!-- <text style='color:#9B9B9B;font-size:28rpx;'>预付定金10元,配送员上门后付洗衣费</text> -->
    </view>

    <view style='display:flex;flex-direction:row;height:20rpx;width:100%;margin-top:20rpx;margin-bottom:20rpx;'></view>    
    <view style='width:100%;display:flex;flex-direction:row;justify-content:space-around;align-items:center;'>
        <view style='display:flex;flex-direction:column;justify-content:center;align-items:center;' bindtap='show_service_flow'>
            <image style='width:42rpx;height:42rpx;border:2rpx solid grey;padding:8rpx;border-radius:10rpx;' src='../../imgs/service/service_flow.png'></image>
            <text style='font-size:24rpx;padding-top:4rpx;'>服务流程</text>
        </view>
        <view style='display:flex;flex-direction:column;justify-content:center;align-items:center;' bindtap='show_service_area'>
            <image style='width:42rpx;height:42rpx;border:2rpx solid grey;padding:8rpx;border-radius:10rpx;' src='../../imgs/service/service_area.png'></image>
            <text style='font-size:24rpx;padding-top:4rpx;'>服务区域</text>
        </view>
        
        <view style='display:flex;flex-direction:column;justify-content:center;align-items:center;' bindtap='make_phone'>
            <image style='width:42rpx;height:42rpx;border:2rpx solid grey;padding:8rpx;border-radius:10rpx;' src='../../imgs/service/about.png'></image>
            <text style='font-size:24rpx;padding-top:4rpx;'>联系我们</text>
        </view>
    </view>
</view>